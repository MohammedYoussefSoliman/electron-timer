{
  "name": "cra-craco-electron-example",
  "version": "0.1.1",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start:web": "env-cmd -f .env.web craco start",
    "start:electron": "electron .",
    "start:electron-dev": "electron electron-dev.js",
    "build": "yarn build:electron",
    "build:web": "craco build",
    "build:electron": "env-cmd -f .env.electron craco build",
    "watch:electron": "npm-watch",
    "test": "craco test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "watch": {
    "build:electron": "src/**/*.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "desktop-idle": "^1.3.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.1.0",
    "electron-is-dev": "^1.2.0",
    "env-cmd": "^10.0.1",
    "formik": "^2.2.6",
    "node-sass": "4.14.1",
    "npm-watch": "^0.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "screenshot-desktop": "^1.12.3",
    "typeface-poppins": "^1.1.13",
    "typescript": "^4.1.2",
    "web-vitals": "^1.1.0",
    "yup": "^0.32.9"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wwlib/cra-craco-electron-example.git"
  },
  "keywords": [
    "react,create-react-app,cra,craco,electron,electron-builder"
  ],
  "author": "Andrew Rapo <andrew@wwlib.org> (http://wwlib.org/)",
  "license": "MIT",
  "devDependencies": {
    "@types/react-redux": "^7.1.16"
  }
}
